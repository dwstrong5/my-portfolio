
<div class="col-11 col-lg-6">
    <ul id="info-links" class="d-flex justify-content-between mx-0 me-auto my-4">
        <li class="nav-item">
            <h3 class="info-link-container">
                <a id="info-link-about" class="nav-link <%= active === 'about' ? 'active' : '' %>" aria-current="page" href="/about">About Me</a>
            </h3>
        </li>
        <li class="nav-item">
            <div class="divider"></div>
        </li>
        <li class="nav-item">
            <h3 class="info-link-container">
                <a id="info-link-education" class="nav-link <%= active === 'education' ? 'active' : '' %>" aria-current="page" href="/education">Education</a>
            </h3>
        </li>
        <li class="nav-item">
            <div class="divider"></div>
        </li>
        <li class="nav-item">
            <h3 class="info-link-container">
                <a id="info-link-experience" class="nav-link <%= active === 'experience' ? 'active' : '' %>" aria-current="page" href="/experience">Experience</a>
            </h3>
        </li>
    </ul>
    <% if(active === 'about' || active === 'education') { %> 
    <% for (let j = 0; j < info.content.length; j++) { %>
        <p style="text-align: justify;"><%= info.content[j] %></p>
    <% } %>
    <% } else if(active === 'experience'){ %>
        <% for (let i = 0; i < info.positions.length; i++) { %>
        <div class="w-100 d-flex flex-column flex-md-row justify-content-md-between">
            <div class="d-flex flex-column flex-md-row mb-0 mb-md-2 align-items-start align-items-md-end">            
                <h6 class="mb-0" style="margin-right: 1rem; font-size: 1.1rem;"><%= info.positions[i]['role-title']%></h6>
                <div id="company"><i><%= info.positions[i]['company']%></i></div>
            </div>
            <div class="mb-2 m-md-0">
                <%= info.positions[i]['role-from']%> - <%= info.positions[i]['role-to']%>
            </div>
        </div>
        <div class="mb-5">
            <% for (let k = 0; k < info.positions[i]['role-duties'].length; k++) { %>
                <p style="margin-bottom: 0.125rem; text-align: justify;">â€¢ <%= info.positions[i]['role-duties'][k]%></p>
            <% } %>
        </div>
    <% } %>
    <% } %>
</div>


